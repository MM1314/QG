<?xml version="1.0"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>

  <connectionStrings>
    <add name="MSGEntities" connectionString="metadata=res://*/SOAWLDB.csdl|res://*/SOAWLDB.ssdl|res://*/SOAWLDB.msl;provider=System.Data.SqlClient;provider connection string=&quot;Data Source=.\SQLEXPRESS;AttachDbFilename=|DataDirectory|\SOAWLDB.mdf;Integrated Security=True;User Instance=True;MultipleActiveResultSets=True&quot;" providerName="System.Data.EntityClient" />
  </connectionStrings>

  <appSettings>
    
    <!--       服务基础信息设置            -->
    
    <!--消息发送类型：邮件Mail;以分号隔开-->
    <add key="MsgSentType" value="Mail;"/>
    <!--消息发送时钟间隔，单位毫秒，默认60000. 注：该时钟间隔应大于网络响应的最长时间，以避免定时器线程异步调用冲突。-->
    <add key="MsgMailInterval" value="1000"/>
   
    <!--         日志记录设置             -->

    <!--服务日志记录方式：记录到文件中 FileLog；记录到windows 事件中 EventLog；默认EventLog-->
    <add key="MsgLogOutput" value="FileLog"/>
    <!--服务日志记录到windows事件时生效，指定日志独立目录名称，文件源名称-->
    <add key="MsgLogName" value="SOA.WLIMS Message"/>
    <add key="MsgLogSource" value="Service Logs"/>
    <!--服务日志目录，记录到文件时生效，默认D:\WLIMSMessageLog-->
    <add key="MsgLogDirectory" value="D:\log"/>

    
    <!--         邮件服务设置             -->
    
    <!--系统邮件服务器 mail.qq.com-->
    <add key="MailServer" value="mail.qq.com"/>
    <!--系统邮箱-->
    <add key="MailAddress" value="1056426706@qq.com"/>
    <!--系统邮箱密码-->
    <add key="MailPassword" value="lvmxj@2014"/>
    <!--邮件发送调用超时时间 单位毫秒 默认1000-->
    <add key="MailTimeOut" value="7000"/>    
    <!--系统邮箱显示名 非必填-->
    <add key="MailDisplayName" value="强哥物流信息管理系统"/>
  </appSettings>

</configuration>
