  <appSettings>
    
    <!--       服务基础信息设置            -->
    
    <!--消息发送类型：邮件Mail；短信SMS；以分号隔开-->
    <add key="MsgSentType" value="Mail;SMS;"/>
    <!--消息发送时钟间隔，单位毫秒，默认60000. 注：该时钟间隔应大于网络响应的最长时间，以避免定时器线程异步调用冲突。-->
