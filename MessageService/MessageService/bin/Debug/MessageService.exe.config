    <add key="MsgSMSInterval" value="60000"/>
    <!--Job时钟间隔（精度设置）。单位毫秒，默认120000. -->
    <add key="JobInterval" value="120000"/>

    
    <!--         日志记录设置             -->

    <!--服务日志记录方式：记录到文件中 FileLog；记录到windows 事件中 EventLog；默认EventLog-->
    <add key="MsgLogOutput" value="FileLog"/>
    <!--服务日志记录到windows事件时生效，指定日志独立目录名称，文件源名称-->
    <add key="MsgLogName" value="Tytus Message"/>
    <add key="MsgLogSource" value="Service Logs"/>
    <!--服务日志目录，记录到文件时生效，默认D:\TytusMessageLog-->
